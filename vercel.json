{
  "buildCommand": "npm install --prefix=frontend/vue-project && npm run build --prefix=frontend/vue-project",
  "outputDirectory": "frontend/vue-project/dist",
  "devCommand": "npm run dev --prefix=frontend/vue-project",
  "installCommand": "npm install",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
